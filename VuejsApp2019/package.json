{
  "name": "vuejs-app2019",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --port 1337",
    "build": "vue-cli-service serve --port 1337",
    "lint": "vue-cli-service lint"
  },
  "description": "VuejsApp2019",
  "author": {
    "name": ""
  },
  "dependencies": {
    "@kurkle/color": "^0.3.2",
    "chart.js": "3.9.1",
    "gsap": "^3.12.2",
    "vue": "2.6.12",
    "vue-chartjs": "4.1.2",
    "vue-graph": "^0.8.7",
    "vue-plugin-load-script": "^2.1.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "4.5.13",
    "@vue/cli-plugin-eslint": "4.5.13",
    "@vue/cli-service": "4.5.13",
    "babel-eslint": "10.1.0",
    "eslint": "6.8.0",
    "eslint-plugin-vue": "6.2.2",
    "vue-template-compiler": "2.6.12"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-unused-vars": [
        "warn",
        {
          "argsIgnorePattern": "^_",
          "varsIgnorePattern": "^_"
        }
      ]
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
